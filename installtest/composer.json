{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "4.2.*",
		"acdoorn/pagemodule": "dev-master"
		// "acdoorn/packagetest": "dev-master",
		// "acdoorn/packagetest2": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"minimum-stability": "stable",
	"repositories": {
		"packagetest": {
			"type": "vcs",
			"url": "https://github.com/acdoorn/packagetest"
		},
		"packagetest2": {
			"type": "vcs",
			"url": "https://github.com/acdoorn/packagetest2"
		},
		"pagemodule": {
			"type": "vcs",
			"url": "https://github.com/acdoorn/pagemodule"
		}
	}
}
